        <mxCell id="n0_sEFOKQVaXVydTx9_c-2" value="&lt;div data-ccp-timestamp=&quot;1702988032285&quot;&gt;&lt;table width=&quot;959&quot; style=&quot;border-collapse:collapse;width:719pt&quot;&gt;&lt;colgroup&gt;&lt;col width=&quot;115&quot; style=&quot;width:86pt&quot;&gt;&lt;col width=&quot;88&quot; style=&quot;width:66pt&quot;&gt;&lt;col width=&quot;756&quot; style=&quot;width:567pt&quot;&gt;&lt;/colgroup&gt;&lt;tbody&gt;&lt;tr height=&quot;20&quot; style=&quot;height:15.0pt&quot;&gt;&lt;td width=&quot;115&quot; height=&quot;20&quot; style=&quot;width:86pt;height:15.0pt;color:white;font-weight:700;background:#A02B93;mso-pattern:#A02B93 none&quot;&gt;TRAZABILIDAD&lt;/td&gt;&lt;td width=&quot;88&quot; style=&quot;width:66pt;color:white;font-weight:700;background:#A02B93;mso-pattern:#A02B93 none&quot;&gt;CODIGO REQUISITO&lt;/td&gt;&lt;td width=&quot;756&quot; style=&quot;width:567pt;color:white;font-weight:700;background:#A02B93;mso-pattern:#A02B93 none&quot;&gt;REQ FUNCIONALES&lt;/td&gt;&lt;/tr&gt;&lt;tr height=&quot;20&quot; style=&quot;height:15.0pt&quot;&gt;&lt;td height=&quot;20&quot; class=&quot;xl16&quot; style=&quot;height:15.0pt;font-family:&amp;quot;Aptos Narrow&amp;quot;, sans-serif;mso-font-charset:1;background:#D9D9D9;mso-pattern:#D9D9D9 none&quot;&gt;REQ_U01&lt;/td&gt;&lt;td class=&quot;xl16&quot; style=&quot;font-family:&amp;quot;Aptos Narrow&amp;quot;, sans-serif;mso-font-charset:1;background:#D9D9D9;mso-pattern:#D9D9D9 none&quot;&gt;REQ_F01&lt;/td&gt;&lt;td style=&quot;background:#D9D9D9;mso-pattern:#D9D9D9 none&quot;&gt;Se podrá ingresar una cantidad de dinero en un tipo de fondo&lt;/td&gt;&lt;/tr&gt;&lt;tr height=&quot;20&quot; style=&quot;height:15.0pt&quot;&gt;&lt;td height=&quot;20&quot; style=&quot;height:15.0pt&quot;&gt;REQ_U01&lt;/td&gt;&lt;td&gt;REQ_F02&lt;/td&gt;&lt;td&gt;Se podrá extraer una cantidad de dinero en un tipo de fondo&lt;/td&gt;&lt;/tr&gt;&lt;tr height=&quot;20&quot; style=&quot;height:15.0pt&quot;&gt;&lt;td height=&quot;20&q